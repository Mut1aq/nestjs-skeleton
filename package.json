{
  "name": "nestjs-skeleton",
  "version": "0.0.1",
  "engines": {
    "node": "18.15.0"
  },
  "description": "starter code for my projects",
  "author": "Mutlaq Alsadeed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/platform-socket.io": "^10.0.5",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^7.0.12",
    "@nestjs/throttler": "^4.1.0",
    "@nestjs/websockets": "^10.0.5",
    "@supercharge/request-ip": "^1.2.0",
    "bcrypt": "^5.1.0",
    "buffer-to-stream": "^1.0.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.37.3",
    "compression": "^1.7.4",
    "ejs": "^3.1.9",
    "express-mongo-sanitize": "^2.2.0",
    "firebase-admin": "^11.9.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "mongoose": "^7.3.1",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@nestjs/cli": "^10.1.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/buffer-to-stream": "^1.0.0",
    "@types/compression": "^1.7.2",
    "@types/cron": "^2.0.1",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.3",
    "@types/nodemailer": "^6.4.8",
    "@types/qrcode": "^1.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
